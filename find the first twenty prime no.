import java.util.Scanner;
class monis
{    
public static void main(String args[])   
{        
Scanner sc=new Scanner(System.in);        
int a=1,i,j,count=0;        
System.out.println("the first 20 prime no. are ");               
for (i = 1; a <= 20; i++)
{count=0;
    for(j = 1; j <= i; j++)
    {
if(i%j==0)
count++;
}
if (count==2)
{        
System.out.println(""+i);
a++;
}  
}
}
}
