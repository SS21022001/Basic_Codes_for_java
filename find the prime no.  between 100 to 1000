import java.util.Scanner;
class prime_100_1000
{    
public static void main(String args[])   
{        
Scanner sc=new Scanner(System.in);        
int i,j,count=0;        
System.out.println("the prime no. between 100 to 1000 are ");               
for (i = 100; i < 1000; i++)
{count=0;
    for(j = 1; j <= i; j++)
    {
if(i%j==0)
count++;
}
if (count==2)
{        
System.out.println(""+i);
}  
}
}
}
