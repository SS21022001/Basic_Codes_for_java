import java.util.Scanner;
class even_odd
{    
public static void main(String args[])   
{        
Scanner sc=new Scanner(System.in);        
int a;        
System.out.println("enter a number= ");        
a=sc.nextInt();        
if(a%2==0)
{        
System.out.println("the no. is even");
}
else
{
System.out.println("the no. is odd");
}    
}
}
