import java.util.Scanner;
class prime
{    
public static void main(String args[])   
{        
Scanner sc=new Scanner(System.in);        
int a,i,count=0;        
System.out.println("enter a number= ");        
a=sc.nextInt();        
for (i = 1; i <= a; i++)
{
if(a%i==0)
count++;
}
if (count==2)
{        
System.out.println("it is a prime no.");
}
else
{
System.out.println("it is not a prime no.");
}    
}
}
